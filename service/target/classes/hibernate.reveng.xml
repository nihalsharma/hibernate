<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC
        "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

  <type-mapping>
    <sql-type jdbc-type="DECIMAL" hibernate-type="Double"/>
    <sql-type jdbc-type="INTEGER" hibernate-type="Long"/>
    <sql-type jdbc-type="TINYINT" hibernate-type="Boolean"/>
  </type-mapping>

  <table-filter match-name="action_queue"/>
  <table-filter match-name="action_item"/>
  <table-filter match-name="classification"/>
  <table-filter match-name="traffic_state"/>
  <table-filter match-name="watcher"/>


</hibernate-reverse-engineering>

